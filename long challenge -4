#include <bits/stdc++.h>
using namespace std;

#define max 1000000


int isPrime[1000001];

void Sieve()
{
    
    //bool prime[1000001];
    bool prime[max+1];
    
    prime[0]=0;
    prime[1]=0;
    int i;
    for(i=2;i<=max;i++)
    prime[i]=1;
    
    //isPrime[max+1]=0;
    
    int p,count=0;
    
    for (p=2;p*p<=max;p++)
    {
        
        if (prime[p]==1) 
        {
             
           
            for (i=p*p;i<=max;i+=p)
                prime[i]=0;
        }
    }
 
    for(p=2;p<=max;p++)
    {
        if(prime[p]==1)
        {
            count++;
        }
        isPrime[p]=count;
    }
}
 

int main()
{
    int t;
    cin>>t;
    Sieve();
    while(t--)
    {
        int x,y;
        cin>>x>>y;
        if(isPrime[x]<=y)
        cout<<"Chef"<<"\n";
        else
        cout<<"Divyam"<<"\n";
    }
    return 0;
}
